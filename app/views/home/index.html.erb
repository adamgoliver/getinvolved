<!-- Carousel
================================================== -->
<div id="myCarousel" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol>
  <div class="carousel-inner">
    <div class="item active">
      <div class="container">
        <%= image_tag("slide1.svg", alt: "First Slide") %>
        <div class="carousel-caption">
          <h1>We connect helpers to other helpers!</h1>
          <p>Whether you have a great cause, or just want to help, you're in the right spot.  We match great "Stories" with great people that are willing to help.</p>
          <p><a class="btn btn-lg btn-primary" href="#" role="button">Sign Up!</a></p>
        </div>
      </div>
    </div>
    <div class="item">
      <%= image_tag("slide1.svg", alt: "Second Slide") %>
      <div class="container">
        <div class="carousel-caption">
          <h1>Lead the Charge!</h1>
          <p>Do you know of a great cause, but need help getting it completed?  You're in the right spot, start a story now.</p>
          <p><a class="btn btn-lg btn-primary" href="#" role="button">Create a Story!</a></p>
        </div>
      </div>
    </div>
    <div class="item">
      <%= image_tag("slide1.svg", alt: "Third Slide") %>
      <div class="container">
        <div class="carousel-caption">
          <h1>Contribute to a Cause!</h1>
          <p>Are you looking for more ways to help others? You're in the right spot, search for stories that need help here.</p>
          <p><a class="btn btn-lg btn-primary" href="#" role="button">Find a Story</a></p>
        </div>
      </div>
    </div>
  </div>
  <a class="left carousel-control" href="#myCarousel" data-slide="prev"><span class="glyphicon glyphicon-chevron-left"></span></a>
  <a class="right carousel-control" href="#myCarousel" data-slide="next"><span class="glyphicon glyphicon-chevron-right"></span></a>
</div><!-- /.carousel -->



